class Foo {
  int x;
  float y;
  int z;
  constructor(int x, float y, int z) {
    this.x = x;
    this.y = y;
    this.z = z;
  }
  int getX() {
    return this.x;
  }
  void setX(int x) {
    this.x = x;
  }

  float getY() {
    return this.y;
  }
  
  void setY(float y) {
    this.y = y;
  }
  
  int getZ() {
    return this.z;
  }

  void setZ(int z) {
    this.z = z;
  }
    
}

void main() {
  Foo my_foo = Foo(2, 5.5, 3);
  printf("x: %d\ny: %f\nz: %d\n", my_foo.getX(), my_foo.getY(), my_foo.getZ());
  my_foo.setX(5);
  my_foo.setY(3.14);
  my_foo.setZ(1);
  printf("x: %d\ny: %f\nz: %d\n", my_foo.getX(), my_foo.getY(), my_foo.getZ());
}
